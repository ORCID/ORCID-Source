    <repositories>
        <!-- temporary repo until 2.0.6 is out -->
        <repository>
            <id>orcid-spring-oauth-copy</id>
            <name>copy of spring security oauth</name>
            <url>https://raw.github.com/ORCID/spring-security-oauth/mvn-repo/</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>spring-social-facebook-orcid-snapshot</id>
            <name>copy of spring-social-facebook to work with Graph API 2.10</name>
            <url>https://raw.github.com/ORCID/spring-social-facebook/mvn-repo/</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>
        <repository>
            <id>maven-restlet</id>
            <name>Public online Restlet repository</name>
            <url>https://maven.restlet.com</url>
        </repository>
        <repository>
            <id>swagger2markup</id>
            <name>Swagger2markup</name>
            <url>https://jcenter.bintray.com</url>
        </repository>
    </repositories>
    <pluginRepositories>
        <pluginRepository>
            <id>m2.java.net</id>
            <name>Java.net Maven 2 Repository</name>
            <url>https://download.java.net/maven/2</url>
            <layout>default</layout>
        </pluginRepository>
        <pluginRepository>
            <id>mc-release</id>
            <name>Local Maven repository of releases</name>
            <url>https://mc-repo.googlecode.com/svn/maven2/releases</url>
            <snapshots>
                <enabled>false</enabled>
            </snapshots>
            <releases>
                <enabled>true</enabled>
            </releases>
        </pluginRepository>
    </pluginRepositories>

    <profiles>
        <!--  Localhost deployments -->
        <profile>
            <id>env-localhost</id>
            <activation>
                <property>
                    <name>env</name>
                    <value>localhost</value>
                </property>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <!-- Same tomcat manager for every web app -->
                <tomcat.manager.url>http://localhost:8080/manager/text</tomcat.manager.url>
                <tomcat.server.id>TomcatServer</tomcat.server.id>
            </properties>
        </profile>

        <!-- QA fullstack deployments -->
        <profile>
            <id>qa-fullstack</id>
            <activation>
                <property>
                    <name>env</name>
                    <value>qa-fullstack</value>
                </property>
            </activation>
            <properties>
                <!-- tomcat.manager.url for each web app is defined in the pom for the web app -->
                <tomcat.server.id>TomcatServer_Qa_Fullstack</tomcat.server.id>
            </properties>
        </profile>
        <profile>
            <id>qa-1</id>
            <activation>
                <property>
                    <name>env</name>
                    <value>qa-1</value>
                </property>
            </activation>
            <properties>
                <!-- tomcat.manager.url for each web app is defined in the pom for the web app -->
                <tomcat.server.id>TomcatServer</tomcat.server.id>
            </properties>
        </profile>

    </profiles>

</project>
