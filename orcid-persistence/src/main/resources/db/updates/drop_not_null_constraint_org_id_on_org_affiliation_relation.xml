<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
        
    <changeSet id="DROP-NOT-NULL-CONSTRAINT-ORG_ID-ON-ORG_AFFILIATION_RELATION" author="Camelia Dumitru" dbms="postgresql">
        <preConditions onFail="MARK_RAN">
            <sqlCheck expectedResult="NO">select is_nullable from information_schema.columns where table_name = 'org_affiliation_relation' and column_name = 'org_id';</sqlCheck>
        </preConditions>
         <sql>ALTER TABLE org_affiliation_relation ALTER COLUMN org_id DROP not null;</sql>         
     </changeSet>
     
     <changeSet id="DDROP-NOT-NULL-CONSTRAINT-ORG_ID-ON-ORG_AFFILIATION_RELATION_HSQL" author="Camelia Dumitru" dbms="hsqldb">
		<dropNotNullConstraint tableName="org_affiliation_relation" columnName="org_id" />
	</changeSet>
	
</databaseChangeLog>