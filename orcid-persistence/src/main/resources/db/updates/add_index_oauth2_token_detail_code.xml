<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-2.0.xsd">
                
    <changeSet id="ADD-INDEX-ON-AUTH2-TOKEN-DETAIL-AUTH-CODE" author="rcpeters">
        <preConditions onFail="MARK_RAN">
            <not>
                <indexExists indexName="oauth2_token_detail_authorization_code_idx" tableName="oauth2_token_detail" />
            </not>
        </preConditions>
        <sql>CREATE INDEX oauth2_token_detail_authorization_code_idx on oauth2_token_detail(authorization_code);</sql>
    </changeSet>
    
</databaseChangeLog>